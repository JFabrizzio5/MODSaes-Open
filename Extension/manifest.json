{
    "manifest_version": 3,
    "name": "ModSaes-Open",
    "version": "0.1",
    "description": "Extension y iniciativa de codigo abierto para mejorar experiencia con el saes ",
    "permissions": [],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"], 
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
 
    }
  }
  
